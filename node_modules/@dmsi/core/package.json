{
  "version": "8.0.0",
  "author": "DMSi Software <miami@dmsi.com>",
  "description": "Core DMSi JavaScript utilities",
  "license": "SEE LICENSE IN LICENSE.md",
  "main": "dist/index.cjs",
  "module": "dist/index.esm.js",
  "exports": {
    "require": "./dist/index.cjs",
    "import": "./dist/index.esm.js"
  },
  "type": "module",
  "types": "dist/index.d.ts",
  "name": "@dmsi/core",
  "repository": "git clone git@bitbucket.org:dmsi-software/dmsi.git",
  "files": [
    "dist"
  ],
  "dependencies": {
    "bowser": "^2.0.0-beta.3",
    "dot-prop": "^7.2.0",
    "is-number": "^7.0.0",
    "numeral": "^2.0.6",
    "pipr": "^1.2.0",
    "prop-types": "^15.6.0",
    "qs": "^6.5.2",
    "reconnecting-websocket": "^3.2.2",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/is-number": "^7.0.1",
    "@types/moment": "^2.13.0",
    "@types/moment-timezone": "^0.5.23",
    "@types/numeral": "^2.0.1",
    "@types/uuid": "^8.3.0",
    "@wedgekit/scripts": "latest",
    "moment-timezone": "^0.5.23"
  },
  "peerDependencies": {
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.23",
    "react": "^16.3.0",
    "relay-compiler": "^1.4.1",
    "relay-runtime": "^1.4.1"
  },
  "scripts": {
    "build": "wedgekit build -p -c",
    "eslint": "eslint src",
    "prepublish": "yarn build",
    "preversion": "yarn build",
    "prepublish:test": "yarn version --prerelease --preid tc --no-git-tag-version",
    "publish:test": "yarn publish --tag test",
    "start": "wedgekit build -p -d -w",
    "test": "ava",
    "test:gha": "c8 ava --tap | tap-xunit > ~/reports/ava.xml",
    "test:coverage": "c8 ava"
  },
  "ava": {
    "files": [
      "src/**/*.test.ts"
    ],
    "timeout": "1m",
    "extensions": {
      "ts": "module"
    },
    "nonSemVerExperiments": {
      "configurableModuleFormat": true
    },
    "nodeArguments": [
      "--no-warnings",
      "--loader=ts-node/esm",
      "--experimental-specifier-resolution=node"
    ],
    "require": [
      "../../test/setup.js"
    ]
  }
}
