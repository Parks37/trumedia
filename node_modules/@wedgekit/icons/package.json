{
  "name": "@wedgekit/icons",
  "version": "4.0.0",
  "main": "dist/icons.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "files": [
    "dist/*",
    "CHANGELOG.md"
  ],
  "repository": "https://design.dmsi.com/",
  "license": "MIT",
  "devDependencies": {
    "camelcase": "^5.3.1",
    "decamelize": "^5.0.0",
    "dot-prop": "^7.2.0",
    "ejs": "^2.6.2",
    "make-dir": "^3.0.0",
    "npm-run-all": "^4.1.5",
    "react": "^17.0.2",
    "rimraf": "^2.6.3",
    "styled-components": "^4.3.1",
    "xml2js": "^0.4.19"
  },
  "scripts": {
    "prebuild": "yarn clean && yarn build:icons",
    "build": "run-s build:es build:tsc",
    "build:icons": "node ./tools/createSVGComponents.js && node ./tools/createSourceMap.js",
    "build:es": "node --experimental-json-modules ./build.js",
    "build:tsc": "tsc",
    "watch:es": "yarn build:es --watch",
    "start": "yarn build && run-p --print-label watch:*",
    "prepublish": "yarn build",
    "prestart": "yarn clean && yarn build:icons",
    "prepublish:test": "yarn version --prerelease --preid tc --no-git-tag-version",
    "publish:test": "yarn publish --tag test",
    "clean": "rimraf dist/ && mkdir dist && rimraf icons/",
    "test:circleci": "echo \"No tests yet\""
  },
  "peerDependencies": {
    "react": "^17.0.2",
    "styled-components": "^4.3.1"
  },
  "dependencies": {
    "@wedgekit/color": "^4.5.3",
    "@wedgekit/primitives": "^4.0.0"
  }
}
