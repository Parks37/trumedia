{
  "author": "DMSi",
  "description": "Wedgekit Design System colors",
  "devDependencies": {
    "shx": "^0.3.4"
  },
  "license": "MIT",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "browser": "dist/index.js",
  "exports": {
    "import": "./dist/index.js",
    "require": "./dist/index.cjs"
  },
  "name": "@wedgekit/palette",
  "scripts": {
    "build": "shx rm -rf dist && yarn build-cjs && yarn build-esm",
    "build-cjs": "tsc -p tsconfig.cjs.json && shx mv dist/index.js dist/index.cjs",
    "build-esm": "tsc -p tsconfig.esm.json",
    "prepublish": "yarn build"
  },
  "type": "module",
  "types": "dist/index.d.ts",
  "version": "1.0.0"
}
